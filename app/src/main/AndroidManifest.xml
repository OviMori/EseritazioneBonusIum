<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eseritazionebonusium">

    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:icon="@drawable/bonus_blue_circle"
        android:label="@string/app_name"
        android:roundIcon="@drawable/bonus_blue_circle"
        android:supportsRtl="true"
        android:theme="@style/Theme.EseritazioneBonusIum">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login.LoginActivity"/>
        <activity android:name=".registration.RegistrationActivity" />
        <activity android:name=".home.HomeActivity" />
        <activity android:name=".changepassword.ModifyPasswordActivity" />
        <activity android:name=".usermanager.UserManagerActivity" />
    </application>

</manifest>